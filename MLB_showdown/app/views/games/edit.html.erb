<div class="container">
  <h2>Edit the Game</h2>
  <%= form_for @game do |f| %>
  <%= f.label :name %>: <%= f.text_field :name %><br />
  <%= f.label :home_team_user_id %>:
  <%= f.text_field :home_team_user_id %><br />
  <%= f.label :visitor_team_user_id %>:
  <%= f.text_field :visitor_team_user_id %><br />
  <%= f.submit %>
  <% end %>
  <%= link_to "Delete this entry", game_path(@game.id), method: 'delete' %>
</div>
